{
	"name": "kitelist",
	"version": "1.0.0",
	"description": "KiteList",
	"author": "cosbgn",
	"private": true,
	"scripts": {
		"dev": "npm run dev_backend & npm run dev_frontend --port 3000",
		"dev_backend": "nodemon --watch api -r dotenv/config ./api/serverless.js",
		"dev_frontend": "nuxt dev --port $port",
		"prebuild": "",
		"build": "nuxt build",
		"start": "nuxt start",
		"analyze": "nuxt build --analyze",
		"generate": "npm run prebuild && nuxt generate --modern",
		"run_static": "nuxt serve",
		"kill": "kill -9 $(lsof -t -i tcp:3000) && killall node && pkill node",
		"netlify-dev": "npx netlify-cli dev",
		"go-live": "npx localtunnel --port 3000 --subdomain kitelist"
	},
	"dependencies": {
		"@google-cloud/firestore": "^4.9.3",
		"@google-cloud/storage": "^5.7.4",
		"@nuxtjs/auth": "^4.9.1",
		"@nuxtjs/axios": "^5.13.1",
		"@nuxtjs/content": "^0.3.0",
		"@nuxtjs/sentry": "^5.0.1",
		"@nuxtjs/tailwindcss": "^3.4.2",
		"autoprefixer": "^9.8.6",
		"nuxt": "^2.15.2",
		"papaparse": "^5.1.0",
		"postcss": "^7.0.35",
		"vue-gtag": "^1.11.0"
	},
	"devDependencies": {
		"@nuxtjs/proxy": "^2.1.0",
		"@sentry/node": "^6.1.0",
		"aws-lambda-fastify": "^1.4.4",
		"fastify": "^3.11.0",
		"fastify-formbody": "^5.0.0",
		"fastify-helmet": "^5.2.0",
		"fastify-rate-limit": "^5.0.1",
		"jsonwebtoken": "^8.5.1",
		"nodemon": "^2.0.7",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
	}
}
